<h2>
  <%= link_to 'Go to HomePage', '/', class:"button button2", method: :get %>
  <%= link_to 'Go to Cars', '/cars', class:"button button2", method: :get %>
  <%= link_to 'Go to Dealerships', '/dealerships', class:"button button2", method: :get %>
</h2>

<hr>

<h1>Add Car to Lot</h1>

<%= form_tag "/dealerships/#{@dealership.id}/cars", method: :post do %>

<p> 
  <%= label_tag :model, 'Model of Car' %>
  <%= text_field_tag :model %><br>

  <%= label_tag :make, 'Make of Car' %>
  <%= text_field_tag :make %><br>

  <%= label_tag :year, 'Year of Car' %>
  <%= text_field_tag :year %><br>

  <%= label_tag :under_100k_miles, 'Under 100k Miles' %>
  <%= text_field_tag :under_100k_miles %><br>

  <%= submit_tag 'Add to Lot' %>
</p>
<% end %>
